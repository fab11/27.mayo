install.packages("fpp")
require("fpp")
am<- read.csv("C:\\Users\\SALA-C26\\Downloads\\amazon.csv",header = FALSE)
amt<-ts(am [,2], start = 2000, frequency = 12)
M1<-auto.arima(amt, seasonal = FALSE)
M1

####Autorregresivo con diferenciacion 1
####.8790t-1
